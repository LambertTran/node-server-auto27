<h1>Customer Images</h1>
<hr/>
<div>
  {{#each customers}}
      <div class="car-item">
        <div class="row">
          <div class="col-12 col-md-6">
            <img src="{{this.paths.[0]}}">
          </div>
          <div class="col-12 col-md-6">
            <div class="row">
              <div class="col-6 col-md-12">
              </div>
              <div class="col-6 col-md-12">
                <form action="/admin/delete/customer/{{this.id}}" method="post">
                  <button type="submit" class="btn btn-danger btn-lg">
                    Delete
                  </button>
                </form>
              </div>
            </div>
          </div>
        </div>
        </div>
  {{/each}}
</div>