<h1>Customer Images</h1>
<hr/>
<div class="row">
  {{#each customers}}
      <div class="col-12 col-sm-6 col-lg-4 col-xl-3 customer-item">
        <div>
          <img src="{{this.paths.[0]}}">        
        </div>
        <div>
          <form action="/admin/delete/customer/{{this.id}}" method="post">
            <button type="submit" class="btn btn-danger btn-lg">
              Delete
            </button>
          </form>
        </div>
      </div>
  {{/each}}
</div>