<h1>DashBoard</h1>
<hr/>
<div>
  {{#each cars}}
      <div class="car-item">
        <div class="row">
          <div class="col-12 col-md-6">
            <img src="{{this.paths.[0]}}">
          </div>
          <div class="col-12 col-md-6">
            <div class="row">
              <div class="col-6 col-md-12">
                <h3>{{this.name}} - {{this.year}}</h3>
              </div>
              <div class="col-6 col-md-12">
                <form action="/admin/delete/{{this.id}}" method="post">
                  <button type="submit" class="btn btn-danger btn-lg">
                    Delete
                  </button>
                  
                </form>
                
              </div>
            </div>
          </div>
        </div>
        </div>
  {{/each}}
</div>